extends ../../layout/layout
block title
	title W-ONE IT 
block content
		#fix-cicrle.fix-cicrle(data-position="bottom",data-step="1") 
			.fix-circle-1.fix-circle-step
				p
					color Полный цикл проекта 
					br
					| по замене: от консультации до внедрения и поддержки
			.fix-circle-2.fix-circle-step
				p
					color Многие теряются в подборе российских решений, ведь мало того, что нужно найти похожий функционал, помимо этого 
						br
						| еще нужно «подружить» новое решение с остальными
			.fix-circle-4.fix-circle-step
				p
					color Акция действует
						br
						| до 30 ноября 2022!
				a.btn-arrow(href="#")
					span Запросить консультацию
					.btn-arrow__circle
						<svg width="13" height="13" viewBox="0 0 13 13" fill="none" xmlns="http://www.w3.org/2000/svg">
							<path d="M11.7854 1.40292H1.40372M11.7854 1.40292V11.7846M11.7854 1.40292L1.40372 11.7846" stroke="white" stroke-width="2"/>
						</svg>
			.fix-circle-3.fix-circle-step
				.fix-circle-3__bg
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
					img(src="img/circle-bg.svg", alt="")
				p
					color Выберите категорию
					| - 
					br
					| мы подберем необходимое решение...
				a.btn-arrow(href="#")
					span Запросить консультацию
					.btn-arrow__circle
						<svg width="13" height="13" viewBox="0 0 13 13" fill="none" xmlns="http://www.w3.org/2000/svg">
